@using CloudMockApi.Admin.Models
@model CloudMockApi.Admin.Models.TenantsHomeViewModel
@{
    ViewBag.Title = "Manage Tenants";
}
<h2>@ViewBag.Title.</h2>
<p class="text-success">@ViewBag.StatusMessage</p>
<div class="row">
    <div class="col-md-12">
        @Html.Partial("_AddTenantPartial", new AddTenantViewModel())
    </div>
    <div class="col-md-12">
        @Html.Partial("_TenantsListPartial", Model.Tenants)
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
